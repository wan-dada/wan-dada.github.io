#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+nunjucks@3.2.4/node_modules/nunjucks/bin/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+nunjucks@3.2.4/node_modules/nunjucks/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+nunjucks@3.2.4/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+nunjucks@3.2.4/node_modules/nunjucks/bin/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+nunjucks@3.2.4/node_modules/nunjucks/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+nunjucks@3.2.4/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/precompile" "$@"
else
  exec node  "$basedir/../../bin/precompile" "$@"
fi
