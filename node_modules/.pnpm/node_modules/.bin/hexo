#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+hexo-cli@4.3.1/node_modules/hexo-cli/bin/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+hexo-cli@4.3.1/node_modules/hexo-cli/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+hexo-cli@4.3.1/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+hexo-cli@4.3.1/node_modules/hexo-cli/bin/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+hexo-cli@4.3.1/node_modules/hexo-cli/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/registry.npmmirror.com+hexo-cli@4.3.1/node_modules:/Users/a1/Github/wan-dada.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hexo-cli/bin/hexo" "$@"
else
  exec node  "$basedir/../hexo-cli/bin/hexo" "$@"
fi
